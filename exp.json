{
  "name": "Tic Tac Toe",
  "description": "Minimal implementation of a 'Tic-Tac-Toe' application using React Native",
  "slug": "react-native-tic-tac-toe",
  "privacy": "public",
  "sdkVersion": "15.0.0",
  "version": "1.0.0",
  "orientation": "portrait",
  "primaryColor": "#3A4767",
  "icon": "https://s13.postimg.org/wflosfalj/logo.png",
  "notification": {
    "icon": "https://s13.postimg.org/wflosfalj/logo.png",
    "color": "#000000",
    "androidMode": "default",
    "androidCollapsedTitle": "#{unread_notifications} new interactions"
  },
  "loading": {
    "icon": "https://s13.postimg.org/wflosfalj/logo.png",
    "backgroundImage": "http://i.imgur.com/zsnPy5z.png",
    "backgroundColor": "#FFF",
    "hideExponentText": true
  },
  "appKey": "main",
  "androidStatusBarColor": "#000000",
  "androidShowExponentNotificationInShellApp": false,
  "packagerOpts": {
    "assetExts": ["ttf"]
  },

   /*Deploy configurations */
  "ios": {
    "bundleIdentifier": "org.rmotr.exp",
  },
  "android": {
    "package": "org.rmotr.exp",
  }
}
